{
  "name": "carmine-sdk",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.cjs",
      "types": "./dist/core/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.cjs",
      "types": "./dist/api/index.d.ts"
    },
    "./governance": {
      "import": "./dist/governance/index.mjs",
      "require": "./dist/governance/index.cjs",
      "types": "./dist/governance/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/**/* --dts --format esm,cjs --clean",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "oxlint --config oxlint.config.json .",
    "generate-abis": "./scripts/generateABIs.sh"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "3.2.4",
    "oxlint": "^1.13.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "decimal.js": "^10.6.0",
    "starknet": "^8.9"
  }
}