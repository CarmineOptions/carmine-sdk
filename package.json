{
  "name": "@carmine-options/sdk",
  "version": "1.1.0",
  "author": {
    "name": "David Vodrážka"
  },
  "description": "TypeScript SDK for interacting with Carmine Options AMM",
  "repository": {
    "type": "git",
    "url": "https://github.com/CarmineOptions/carmine-sdk"
  },
  "main": "dist/index.cjs",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.cjs",
      "types": "./dist/core/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.cjs",
      "types": "./dist/api/index.d.ts"
    },
    "./governance": {
      "import": "./dist/governance/index.mjs",
      "require": "./dist/governance/index.cjs",
      "types": "./dist/governance/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/**/* --dts --format esm,cjs --clean",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "oxlint --config oxlint.config.json .",
    "generate-abis": "./scripts/generateABIs.sh"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "3.2.4",
    "oxlint": "^1.13.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "decimal.js": "^10.6.0",
    "starknet": "^8.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "carmine",
    "starknet",
    "defi",
    "sdk",
    "options",
    "web3"
  ]
}